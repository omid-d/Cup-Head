<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import java.net.URL?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="View.ScoreBoardController"
            prefHeight="720" prefWidth="1280" stylesheets="@../css/style.css" fx:id="border">
    <top>
        <GridPane fx:id="grid" alignment="CENTER">

        </GridPane>

    </top>

    <center>

        <VBox spacing="10" alignment="CENTER" maxWidth="350" fx:id="vbox">
            <Text  text="Score Board" styleClass="title"/>

        </VBox>

    </center>

    <bottom>

            <Button layoutX="500"  styleClass="secondary-btn" text="go back" onMouseClicked="#goBack"/>

    </bottom>


    <stylesheets>
        <URL value="@../css/style.css" />
    </stylesheets>
</BorderPane>

